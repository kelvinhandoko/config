    "files.autoSave": "afterDelay",
    "material-ui-snippets.showNotesOnStartup": false,

    "terminal.integrated.stickyScroll.enabled": true,
    "editor.stickyScroll.enabled": true,

    "[plantuml]": {
        "editor.defaultFormatter": "jebbs.plantuml",
        "editor.formatOnSave": false
    },

    "tailwindCSS.experimental.classRegex": [
        ["cn\\(([^)]*)\\)", "(?:'|\"|`)([^'\"`]*)(?:'|\"|`)"],
        ["cva\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"],
        ["cx\\(([^)]*)\\)", "(?:'|\"|`)([^'\"`]*)(?:'|\"|`)"]
    ],
    "tailwindCSS.classAttributes": [
        "class",
        "className",
        "ngClass",
        "class:list",
        "thumbClassName"
    ],

    "markdown-preview-enhanced.revealjsTheme": "moon.css",
    "markdown-preview-enhanced.codeBlockTheme": "github-dark.css",
    "markdown-preview-enhanced.previewTheme": "github-dark.css",
    "github.copilot.editor.enableAutoCompletions": true,
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    },
    "workbench.editor.customLabels.patterns": {
        "**/{layout,page}.tsx": "${dirname} ${filename}"
    },
    "workbench.editor.enablePreview": false,
    "workbench.sideBar.location": "right",
    "editor.minimap.enabled": false,
    "breadcrumbs.enabled": false
}
